(defn vec_map [ f l ]
  (if (= l [])
    []
    (let [ h (head l) t (tail l) ]
      (concat [(f h)] (map f t))
    )
  )
)

(defn vec_filter [ f l ]
  (if (= l [])
    []
    (let [ h (head l) t (tail l) ]
      (if (f h)
        (concat [h] (filter f t))
        (filter f t)
      )
    )
  )
)

(defn vec_foldl [ f acc l ]
  (if (= l [])
    acc
    (let [ h (head l) t (tail l) ]
      (foldl f (f h acc) t)
    )
  )
)

